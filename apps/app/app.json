{
  "expo": {
    "name": "Tugas Collecter",
    "slug": "tugas-collecter",
    "description": "A mobile application for managing your school homework and tasks.",
    "owner": "hexaa",
    "version": "1.0.0",
    "platforms": ["android"],
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "tugascollecter",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icon_foreground.png",
        "backgroundColor": "#3b82f6"
      },
      "package": "sh.hexaa.tugascollecter",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE"
      ],
      "notification": {
        "icon": "./src/assets/images/icon_foreground.png",
        "color": "#3b82f6"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/icon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-notifications"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b2650d55-d0b1-470d-bc43-066b44ff6bdd"
      }
    }
  }
}
