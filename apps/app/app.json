{
  "expo": {
    "name": "tugascollecter",
    "slug": "tugas-collecter",
    "description": "A mobile application for managing your school homework and tasks.",
    "owner": "hexaa",
    "version": "1.0.0",
    "platforms": [
      "android"
    ],
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "tugascollecter",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icon_black.png",
        "backgroundColor": "#ffffff"
      },
      "package": "sh.hexaa.tugascollecter"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b2650d55-d0b1-470d-bc43-066b44ff6bdd"
      }
    }
  }
}
